filter {
  if [fileset][name] == "syslog" {
    grok {
      match => { "message" => [
        "%{SYSLOGLINE}",
        "%{CRONLOG}",
        "%{SYSLOGPAMSESSION}"
        ]
      }
      overwrite => ["message"]
    }
    date {
      match => [ "timestamp", "MMM  d HH:mm:ss", "MMM dd HH:mm:ss" ]
      remove_field => "timestamp"
    }
  }
}
